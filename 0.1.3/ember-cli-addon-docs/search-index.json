docs/examples/chart-loading":{"id":"template:docs/examples/chart-loading","type":"template","title":"Loading","text":"LoadingWhen the chart is ready hide the loading mask ","route":"docs.examples.chart-loading","keywords":[]},"template:docs/examples/chart-simple/demo":{"id":"template:docs/examples/chart-simple/demo","type":"template","title":null,"text":" .echarts-chart { width: 100%; height: 500px; } ","route":"docs.examples.chart-simple.demo","keywords":[]},"template:docs/examples/chart-simple":{"id":"template:docs/examples/chart-simple","type":"template","title":"Simple","text":"SimpleEchart line-chart ","route":"docs.examples.chart-simple","keywords":[]},"template:docs/examples/chart-themes/demo1":{"id":"template:docs/examples/chart-themes/demo1","type":"template","title":null,"text":" .echarts-chart { width: 100%; height: 500px; } ","route":"docs.examples.chart-themes.demo1","keywords":[]},"template:docs/examples/chart-themes/demo2":{"id":"template:docs/examples/chart-themes/demo2","type":"template","title":null,"text":" .echarts-chart { width: 100%; height: 500px; } ","route":"docs.examples.chart-themes.demo2","keywords":[]},"template:docs/examples/chart-themes":{"id":"template:docs/examples/chart-themes","type":"template","title":"Themes","text":"ThemesCustom theme Echart Dark theme ","route":"docs.examples.chart-themes","keywords":[]},"template:docs/examples/index":{"id":"template:docs/examples/index","type":"template","title":"All Examples","text":"All ExamplesSimple Loading Events ThemesCustom theme echart dark theme Dynamic Graph Calendar","route":"docs.examples.index","keywords":[]},"template:docs":{"id":"template:docs","type":"template","title":null,"text":" ","route":"docs","keywords":[]},"template:docs/usage":{"id":"template:docs/usage","type":"template","title":"Installation","text":"An EmberJS echarts wrapper Installationember install ember-cli-echarts Basic UsageAll you need to get started is to pass in the data it should display via the option parameter. {{echarts-chart option=option}} OptionsChart OptionsAll chart specific options can be specified via the option parameter. // Basic area chart option = { xAxis: { type: 'category', boundaryGap: false, data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'] }, yAxis: { type: 'value' }, series: [{ data: [820, 932, 901, 934, 1290, 1330, 1320], type: 'line', areaStyle: {} }] }; {{echarts-chart option=option}} EventsEcharts events API onEvents: { click(param, echart) { console.log(param, echart); alert('chart clicked'); }, legendselectchanged(param, echart){ console.log(param, echart); alert('chart legendselectchanged'); } } {{echarts-chart option=option onEvents=onEvents}} LoadingEcharts showLoading API _t: null, loadingOptions: { text: '加载中...', color: '#4413c2', textColor: '#270240', maskColor: 'rgba(194, 88, 86, 0.3)', zlevel: 0 }, onChartReady(chart) { set(this, '_t', setTimeout(() => { chart.hideLoading(); }, 3000)); }, {{echarts-chart option=option onChartReady=(action onChartReady) loadingOptions=loadingOptions showLoading=true }} ThemesCustom theme init() { this._super(...arguments); this.registerTheme(); set(this, 'theme', 'my_theme'); }, registerTheme() { const colorPalette = [ '#c12e34','#e6b600','#0098d9','#2b821d', '#005eaa','#339ca8','#cda819','#32a487' ]; echarts.registerTheme('my_theme', { \"color\": colorPalette, \"title\": { \"left\": \"center\", \"y\": \"10\", \"textStyle\": { \"color\": \"#fff\" }, }, legend: { \"textStyle\": { \"color\": \"#fff\" }, }, \"backgroundColor\": \"#1c2e40\", }); } Echarts Dark theme init() { this._super(...arguments); // echart default themes set(this, 'theme', 'dark'); } {{echarts-chart option=option theme=theme }} Additional ConfigEcharts init API opts: { devicePixelRatio: 1, renderer: 'svg' // canvas of svg width: '500px', height: '500px' } {{echarts-chart option=option opts=opts }} Helper ActionsCurrently there are two helper actions provided: 1. beforeSetupThis is called before echarts has been initialized. beforeSetup(context, chart) {} {{echarts-chart option=option beforeSetup=(action beforeSetup)}} 2. afterSetupThis is called after all chart options and data have been set, as well as after the chart has been created. afterSetup(context, chart) {} {{echarts-chart option=option afterSetup=(action afterSetup)}} ","route":"docs.usage","keywords":[]},"template:not-found":{"id":"template:not-found","type":"template","title":"Not found","text":" Not found This page doesn't exist. Head home? ","route":"not-found","keywords":[]}}}
